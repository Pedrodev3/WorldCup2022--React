package br.com.fiap.to;

import javax.xml.bind.annotation.XmlRootElement;


@XmlRootElement
public class UsuarioTO {
	
	private String login;
	private String senha;
	private int rm;
	private String nome;

	public UsuarioTO() {
		// TODO Auto-generated constructor stub
	}

	public UsuarioTO(String login, String senha, int rm, String nome) {
		super();
		this.login = login;
		this.senha = senha;
		this.rm = rm;
		this.nome = nome;
	}
	
	

}
