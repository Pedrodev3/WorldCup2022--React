			listaUsuario = new ArrayList<UsuarioTO>();
			UsuarioTO user = new UsuarioTO();
			user.setLogin("Daniel");
			user.setSenha("Daniel123");
			user.setRm(94615);
			user.setNome("Daniel Henrique");
			listaUsuario.add(user);
			user = new UsuarioTO();
			user.setLogin("Daniel");
			user.setSenha("Daniel123");
			user.setRm(94615);
			user.setNome("Daniel Henrique");
			listaUsuario.add(user);
			user = new UsuarioTO();
			user.setLogin("3");
			user.setSenha("3");
			listaUsuario.add(user);
			user = new UsuarioTO();
			user.setLogin("4");
			user.setSenha("4");
			listaUsuario.add(user);
			user = new UsuarioTO();
			user.setLogin("5");
			user.setSenha("5");
			listaUsuario.add(user);
		}

	}
}
